			<if test="busId != null" >
				<![CDATA[ AND bus_id = #{busId,jdbcType=BIGINT} ]]> 
			</if>
			<if test="signingUserId != null" >
				<![CDATA[ AND signing_user_id = #{signingUserId,jdbcType=VARCHAR} ]]> 
			</if>
			<if test="proName != null" >
				<![CDATA[ AND pro_name = #{proName,jdbcType=VARCHAR} ]]> 
			</if>
			<if test="state != null" >
