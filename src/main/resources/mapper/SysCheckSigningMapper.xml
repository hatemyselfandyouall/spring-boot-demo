	<update id="updateSysCheckSigning" parameterType="com.wangxinenpu.springbootdemo.dataobject.po.SysCheckSigning" >
		<!--
		  @haoxz11MyBatis
		  更新待签领列表记录
		-->
		update sys_check_signing
		<set >
			<if test="proId != null" >
				pro_id = #{proId,jdbcType=BIGINT},
			</if>
			<if test="state != null" >
				state = #{state,jdbcType=BIGINT},
			</if>
			<if test="busId != null" >
				bus_id = #{busId,jdbcType=BIGINT},
			</if>
			<if test="signingUserId != null" >
				signing_user_id = #{signingUserId,jdbcType=VARCHAR},
			</if>
			<if test="proName != null" >
				pro_name = #{proName,jdbcType=VARCHAR},
			</if>
			modify_time = sysdate()
		</set>
		where id = #{id,jdbcType=BIGINT}
	</update>
	<select id="getByPrimaryKey" resultMap="SysCheckSigning" parameterType="java.lang.Long" >
