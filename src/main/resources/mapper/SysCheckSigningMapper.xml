	<select id="getListByCondition" resultMap="SysCheckSigning" parameterType="map" >
     SELECT min( t1.pro_id ) pro_id,t1.id,t1.bus_id,t1.pro_name,t1.signing_user_id,t1.state,t1.create_time,t1.modify_time FROM
	  ( SELECT * FROM sys_check_signing t <include refid="whereSQL" /> ) t1 GROUP BY t1.bus_id
	</select>

